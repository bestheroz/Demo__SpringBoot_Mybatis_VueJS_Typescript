{
  "name": "vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest ../src/main/resources/static/ --mode production",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@types/lodash": "latest",
    "axios": "latest",
    "core-js": "~2",
    "fibers": "latest",
    "register-service-worker": "latest",
    "vue": "latest",
    "vue-class-component": "latest",
    "vue-moment": "latest",
    "vue-property-decorator": "latest",
    "vue-router": "latest",
    "vuelidate": "latest",
    "vuetify": "latest",
    "vuex": "latest"
  },
  "devDependencies": {
    "@mdi/font": "latest",
    "@types/vuelidate": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@vue/cli-plugin-babel": "latest",
    "@vue/cli-plugin-eslint": "latest",
    "@vue/cli-plugin-pwa": "latest",
    "@vue/cli-plugin-typescript": "latest",
    "@vue/cli-service": "latest",
    "@vue/eslint-config-prettier": "latest",
    "@vue/eslint-config-typescript": "latest",
    "babel-eslint": "latest",
    "eslint": "~5",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-vue": "latest",
    "sass": "~1",
    "sass-loader": "~7",
    "typescript": "latest",
    "vue-cli-plugin-vuetify": "latest",
    "vue-template-compiler": "latest",
    "vuetify-loader": "latest"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
